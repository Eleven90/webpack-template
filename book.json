{
  "root": "./docs/webpack",
  "title": "webpack-template",
  "author" : "Eleven",
  "links" : {
    "sidebar" : {
        "Eleven" : "https://github.com/Eleven90"
    }
  },
  "language" : "zh-hans",
  "plugins": [
    "-sharing",
    "-lunr",
    "-search",
    "github-buttons",
    "simple-page-toc",
    "ace",
    "splitter",
    "anchors",
    "back-to-top-button",
    "theme-vuejs-2",
    "favicon",
    "pageview-count",
    "lightbox",
    "tbfed-pagefooter",
    "hide-element",
    "copy-code-button",
    "search-pro-fixed"
  ],
  "pluginsConfig": {
    "github-buttons": {
      "buttons": [{
        "user": "Eleven90",
        "repo": "webpack-template",
        "type": "star",
        "count": true,
        "size": "small"
      }]
    },
    "simple-page-toc": {
      "maxDepth": 3,
      "skipFirstH1": true
    },
    "fontsettings": {
      "theme": "white",
      "family": "serif"
    },
    "favicon":{
      "shortcut": "./assets/images/favicon.ico",
      "bookmark": "./assets/images/favicon.ico"
    },
    "tbfed-pagefooter": {
      "modify_label": "Last modified timeï¼š",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "hide-element": {
      "elements": [".gitbook-link", ".copyright"]
    }
  }
}