{
  "name": "webpack-template",
  "version": "1.0.0",
  "description": "webpack-template",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env BUILD_ENV=development USE_HMR=true webpack-dev-server --hot --config build/webpack.config.dev.js --mode development --colors --profile",
    "dev-mock": "cross-env IS_MOCK=true npm run dev",
    "dev:page-a": "cross-env entry=page-a npm run dev",
    "dev:page-b": "cross-env entry=page-b npm run dev",
    "build-dev": "cross-env BUILD_ENV=development webpack --config build/webpack.config.build.js --mode production --colors --profile",
    "build-test": "cross-env BUILD_ENV=test webpack --config build/webpack.config.build.js --mode production --colors --profile",
    "build": "cross-env BUILD_ENV=production webpack --config build/webpack.config.build.js --mode production --colors --profile",
    "watch-dev": "cross-env BUILD_ENV=development IS_WATCH=true webpack -w --config build/webpack.config.watch.js --mode production --colors --profile",
    "watch-test": "cross-env BUILD_ENV=test IS_WATCH=true webpack -w --config build/webpack.config.watch.js --mode production --colors --profile",
    "watch": "cross-env BUILD_ENV=production IS_WATCH=true webpack -w --config build/webpack.config.watch.js --mode production --colors --profile",
    "http-server": "http-server dist"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Eleven90/webpack-template.git"
  },
  "keywords": [
    "webpack-template"
  ],
  "author": "Eleven",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "art-template": "^4.13.2",
    "art-template-loader": "^1.4.3",
    "babel-loader": "^8.0.4",
    "cache-loader": "^4.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.1",
    "cssnano": "^4.1.7",
    "exports-loader": "^0.7.0",
    "file-loader": "^2.0.0",
    "glob": "^7.1.3",
    "happypack": "^5.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mockjs-async": "0.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "postcss-safe-parser": "^4.0.1",
    "script-loader": "^0.7.2",
    "style-loader": "^0.23.1",
    "template-string-optimize-loader": "^3.0.0",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.1.2",
    "vconsole": "^3.2.0",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.5"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "axios": "^0.18.0",
    "moment": "^2.23.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "proxy": {
    "/api-dev-open": {
      "target": "http://open.test.ximalaya.com",
      "secure": true,
      "changeOrigin": true,
      "pathRewrite": {
        "^/api-dev-open": ""
      }
    },
    "/api-dev-m": {
      "target": "http://m.test.ximalaya.com",
      "secure": true,
      "changeOrigin": true,
      "pathRewrite": {
        "^/api-dev-m": ""
      }
    }
  }
}
