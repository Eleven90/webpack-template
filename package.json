{
  "name": "webpack-template",
  "version": "1.0.0",
  "description": "webpack-template",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --config build/webpack.config.dev.js --mode development --colors --profile",
    "te-build": "cross-env NODE_ENV=test webpack --config build/webpack.config.prod.js --mode production --colors --profile",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.config.prod.js --mode production --colors --profile",
    "watch:dev": "cross-env NODE_ENV=development IS_WATCH=watch webpack -w --config build/webpack.config.watch.js --mode production --colors --profile",
    "watch:prod": "cross-env NODE_ENV=production IS_WATCH=watch webpack -w --config build/webpack.config.watch.js --mode production --colors --profile"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "webpack-template"
  ],
  "author": "Eleven",
  "license": "ISC",
  "devDependencies": {
    "art-template": "^4.13.2",
    "art-template-loader": "^1.4.3",
    "autoprefixer": "^9.4.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.1",
    "cssnano": "^4.1.7",
    "exports-loader": "^0.7.0",
    "file-loader": "^2.0.0",
    "glob": "^7.1.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "script-loader": "^0.7.2",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "vconsole": "^3.2.0",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.5"
  },
  "proxy": {
